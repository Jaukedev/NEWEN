            if v= 1 global.POTAS[n,v]= ini_read_real("potas",string(n*10 + v),2)
            if v= 2 global.POTAS[n,v]= ini_read_real("potas",string(n*10 + v),3)
            
        }
        else global.POTAS[n,v]= ini_read_real("potas",string(n*10 + v),0)
    }
}


